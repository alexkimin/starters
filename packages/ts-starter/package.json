{
  "name": "ts-starter",
  "version": "1.0.0",
  "author": "Alex Min",
  "description": "project",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.SERVER_ENV=local --config ./config/webpack/webpack.dev.js",
    "start:sit": "cross-env NODE_ENV=development webpack-dev-server --env.SERVER_ENV=sit --config ./config/webpack/webpack.dev.js",
    "start:uat": "cross-env NODE_ENV=development webpack-dev-server --env.SERVER_ENV=uat --config ./config/webpack/webpack.dev.js",
    "start:comp": "cross-env NODE_ENV=development styleguidist server --config ./config/styleguidist/styleguide.config.js",
    "build": "run-s clean test build:only",
    "build:only": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.prod.js",
    "build:ba": "cross-env NODE_ENV=production webpack --env.analysis --config ./config/webpack/webpack.prod.js",
    "build:comp": "cross-env NODE_ENV=development styleguidist build --config ./config/styleguidist/styleguide.config.js",
    "test": "jest --config config/jest/jest.config.js --changedSince=master",
    "test:watch": "npm run test -- --watch",
    "test:cov": "jest --config config/jest/jest.config.js --coverage",
    "test:open": "run-p test:open:*",
    "test:open:unit": "opener coverage/lcov-report/index.html",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .cache",
    "clean:build": "rimraf dist",
    "clean:test": "run-p clean:test:*",
    "clean:test:unit": "rimraf coverage",
    "validate": "lint-staged",
    "outdated": "npm outdated",
    "update": "npm update",
    "preinstall": "rm -rf node_modules",
    "postinstall": "npm run clean",
    "install:ext": "code --install-extension ./config/vscode-styled-components-fork-0.0.21.vsix --install-extension esbenp.prettier-vscode --install-extension editorconfig.editorconfig --install-extension eg2.tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{ts,tsx}": [
        "npm run test -- --bail --findRelatedTests",
        "stylelint --fix",
        "prettier --print-width=80 --write",
        "tslint",
        "git add"
      ],
      "*.{html,json,js}": [
        "prettier --print-width=80 --write",
        "git add"
      ]
    },
    "ignore": [
      "**/assets/**/*.ts",
      "**/assets/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "antd": "3.12.1",
    "axios": "0.18.0",
    "connected-react-router": "6.1.0",
    "cuid": "2.1.4",
    "date-fns": "1.30.1",
    "detect-browser": "4.0.3",
    "history": "4.7.2",
    "immer": "1.10.0",
    "lodash": "4.17.11",
    "query-string": "6.2.0",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0",
    "react-pdf": "4.0.2",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "react-text-mask": "5.4.3",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-act": "1.7.4",
    "redux-form": "8.1.0",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "smoothscroll-polyfill": "0.4.3",
    "styled-components": "4.1.3",
    "styled-system": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/cuid": "1.3.0",
