{
  "name": "js-starter",
  "version": "1.0.0",
  "author": "Alex Min",
  "description": "project",
  "license": "MIT",
  "main": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --env.SERVER_ENV=local --config ./config/webpack/webpack.dev.js",
    "build": "run-s clean test build:only",
    "build:only": "NODE_ENV=production webpack --config ./config/webpack/webpack.prod.js",
    "build:analysis": "NODE_ENV=production webpack --env.analysis --config ./config/webpack/webpack.prod.js",
    "test": "jest --config config/jest/jest.config.js --changedSince=master",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test:e2e": "npm run cypress:run",
    "test:e2e:gui": "start-server-and-test start http://localhost:3000 cypress:open",
    "test:e2e:local": "start-server-and-test start http://localhost:3000 cypress:run",
    "test:open": "run-p test:open:*",
    "test:open:unit": "opener coverage/lcov-report/index.html",
    "test:open:e2e": "opener e2e_result/reporter/e2e_report.html",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .cache",
    "clean:build": "rimraf dist",
    "clean:test": "run-p clean:test:*",
    "clean:test:unit": "rimraf coverage",
    "clean:test:e2e": "rimraf cypress/screenshots & rimraf e2e_result",
    "validate": "lint-staged",
    "outdated": "npm outdated",
    "update": "npm update",
    "preinstall": "rm -rf node_modules",
    "postinstall": "npm run clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx}": [
        "npm run test -- --bail --findRelatedTests",
        "stylelint --fix",
        "prettier --write",
        "eslint",
        "git add"
      ],
      "*.{html,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/assets/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "connected-react-router": "^6.1.0",
    "cuid": "^2.1.4",
    "date-fns": "^1.30.1",
    "detect-browser": "^3.0.1",
    "history": "^4.7.2",
    "immer": "^8.0.1",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-act": "^1.7.4",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.3",
    "styled-components": "^4.1.3",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.2",
    "cypress": "^3.1.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "error-overlay-webpack-plugin": "^0.1.5",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "ip": "^1.1.5",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^7.0.0",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.5.1",
    "terser-webpack-plugin": "^1.2.0",
    "thread-loader": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-simple-progress-plugin": "^0.0.4"
  }
}
